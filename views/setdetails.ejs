<!-- home.ejs -->
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Home</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    <style>
        .card-img {
            height: 150px;
            /* Adjust the height as needed */
            object-fit: fill;
            /* Ensure the image covers the entire space */
        }

        .symbol-img {
            height: 35px;
            /* Adjust the height as needed */
            object-fit: fill;
            /* Ensure the image covers the entire space */
        }
    </style>
</head>

<body>
    <!-- Include header if needed -->
    <%- include('partials/header') %>

        <div class="container">
            <h1>
                <%= set.name %> Details
            </h1>
            <!-- Display set details -->
            <div>
                <img src="<%= set.logo %>" alt="<%= set.name %> Logo" class="card-img">

                <div class="container text-center">
                    <div class="row">
                        <div class="col">Card Count Official: <%= set.cardCountOfficial %> </div>
                        <div class="col">Card Count Total: <%= set.cardCountTotal %></div>
                        <div class="col">col</div>
                        <div class="col">col</div>
                    </div>
                    <div class="row">
                        <div class="col">col</div>
                        <div class="col">col</div>
                        <div class="col">col</div>
                        <div class="col">col</div>
                    </div>
                    <div class="row">
                        <div class="col">col</div>
                        <div class="col">col</div>
                        <div class="col">col</div>
                        <div class="col">col</div>
                    </div>
                    <!-- Include other set details as needed -->
                </div>
            </div>

            <!-- Include footer if needed -->
            <%- include('partials/footer') %>
                <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
                    integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
                    crossorigin="anonymous"></script>
</body>

</html>